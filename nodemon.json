{
    "restartable": "rs",
    "ignore": [
        ".git",
        ".svn",
        "src/**/*.spec.ts",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "watch": ["src/**/*.ts"],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts",
    "exec": "node -r ts-node/register src/WebServer.ts"
}
